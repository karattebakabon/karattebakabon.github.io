{
  "tools": {
    "autoAccept": false,
    "core": [
      "read_file",
      "write_file",
      "edit_file",
      "run_shell_command"
    ],
    "exclude": [
      "delete_file",
      "Bash(rm -rf /)",
      "Bash(rm -rf ~)",
      "Bash(rm -rf ~/*)",
      "Bash(rm -rf /*)"
    ],
    "sandbox": false
  },
  "general": {
    "checkpointing": {
      "enabled": true
    },
    "preferredEditor": "cursor"
  },
  "context": {
    "fileName": [
      "GEMINI.md"
    ],
    "fileFiltering": {
      "respectGitIgnore": true,
      "enableRecursiveFileSearch": true
    }
  },
  "ui": {
    "theme": "GitHub"
  }
}